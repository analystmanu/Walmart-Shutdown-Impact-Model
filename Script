Let's first do the first step of ETL, that is the extract part.
from google.cloud import storage
import pandas as pd
import io

BUCKET_NAME = "walmart_shutdown_unprocessed"

def read_csv_from_gcs(file_name):
    client = storage.Client()
    bucket = client.bucket(BUCKET_NAME)
    blob = bucket.blob(file_name)
    data_bytes = blob.download_as_bytes()
    df = pd.read_csv(io.BytesIO(data_bytes))
    return df

# Load all files
df_train = read_csv_from_gcs("train.csv")
df_stores = read_csv_from_gcs("stores.csv")
df_features = read_csv_from_gcs("features.csv")
df_test = read_csv_from_gcs("test.csv")

# Quick check
print(df_train.head())
print(df_stores.head())
print(df_features.head())

